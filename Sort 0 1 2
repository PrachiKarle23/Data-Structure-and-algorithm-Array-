#include <bits/stdc++.h> 
void sort012(int *arr, int n)
{
   vector<int>ans;
  for(int i=0;i<n;i++)
  {
     if(arr[i]==0)
     {
        ans.push_back(arr[i]);
     }
  }
  for(int i=0;i<n;i++){
     if(arr[i]==1)
     {
        ans.push_back(arr[i]);
     }
  }
  for(int i=0;i<n;i++)
  {
     if(arr[i]==2)
     {
        ans.push_back(arr[i]);
     }
  }

  for(int i=0;i<n;i++)
  {
     arr[i]=ans[i];
  }
}
