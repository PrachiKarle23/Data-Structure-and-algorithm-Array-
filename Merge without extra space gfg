
            int i=0, j=0, k=m-1;
            while(i<=k && j<n) {
                if(arr1[i]<arr2[j]){
                    i++;
                }
                else {
                    swap(arr2[j], arr1[k]);
                    k--;
                    j++;
                }
            }
            sort(arr1, arr1+m);
            sort(arr2, arr2+n);
